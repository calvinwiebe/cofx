{
  "baseUrl": "types",
  "typeRoots": ["types"],
  "compilerOptions": {
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "baseUrl": ".",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2015", "es2017", "dom"],
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./test/*.ts"
  ]
}
